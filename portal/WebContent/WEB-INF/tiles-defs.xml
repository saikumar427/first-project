</definition>
<definition name=".eventspage" extends=".beeletspage" >
	<put name="head"         value="/main/tabsheader.jsp" />
	<put name="leftdivid"         value="wideleftcolumn" />
	<put name="rightdivid"         value="narrowrightcolumn" />
      <putList name="leftdisplaylist">
			<bean classtype="com.eventbee.web.presentation.beans.BeeletItem" >
        			<set-property property="beeletId" value="eventslist" />
        			<set-property property="isCollapsable" value="false" />
				<set-property property="includeType" value="" />
				<set-property property="beeletBorderType" value="beelettable" />				
				<set-property property="resource" value="/eventdetails/eventscategories.jsp" />
			</bean>
			
		</putList>
		
		<putList name="rightdisplaylist">
			<bean classtype="com.eventbee.web.presentation.beans.BeeletItem" >
