<tiles-definitions>
<definition name=".beeletspage" path="/templates/beeletspagelayout.jsp">
	<put name="head"         value="/main/header.jsp" />
	<put name="mytabs"         value="" />
	<put name="footer"       value="/main/footer.jsp" />
	<put name="leftdivid"         value="wideleftcolumn" />
	<put name="rightdivid"         value="narrowrightcolumn" />
</definition> 

<definition name=".taskpage" path="/templates/taskpagelayout.jsp">
