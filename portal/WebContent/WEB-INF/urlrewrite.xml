<urlrewrite>

<rule>
	    	<name>redirect from xyz.com to www.xyz.com </name>
	    
	    	<condition name="host" operator="notequal">^www\.</condition>
	    	<condition type="request-url">^http://eventbee\.com/</condition>
	    	<from>^/</from>
	    	<to type="redirect">http://www.eventbee.com/</to>
</rule>

